<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">


<generatorConfiguration>

    <context id="DB2Tables" targetRuntime="MyBatis3Simple">
        <commentGenerator>
            <!-- 去掉注释 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!--数据库连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/limestone"
                        userId="root"
                        password="">
        </jdbcConnection>
        <!--默认false
           Java type resolver will always use java.math.BigDecimal if the database column is of type DECIMAL or NUMERIC.
         -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建  使用Maven生成在target目录下，会自动创建） -->
        <javaModelGenerator targetPackage="com.dabigao.limestone.bean.entity"
                            targetProject="/home/king/work/code/java/king/mybatis-generator/generator/core/stonehenge-mybatis-generator-core/generated-sources">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
            <property name="rootClass" value="com.bornstone.stonehenge.entity.BaseEntity"/>
        </javaModelGenerator>
        <!--生成SQLMAP文件 -->
        <sqlMapGenerator targetPackage="mybatis"
                         targetProject="/home/king/work/code/java/king/mybatis-generator/generator/core/stonehenge-mybatis-generator-core/generated-sources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.dabigao.limestone.bean.dao"
                             targetProject="/home/king/work/code/java/king/mybatis-generator/generator/core/stonehenge-mybatis-generator-core/generated-sources">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--对应数据库表 mysql可以加入主键自增 字段命名 忽略某字段等-->
        <!-- useActualColumnNames属性是为了使生成的model字段与数据库的字段大小写不一致。 默认为 false -->

        <!-- Table config begin -->
        <!--
            默认值：
            enableSelectAll:false
            enableDelete:true
        -->
        <table tableName="draft_collect" domainObjectName="DraftCollect" enableSelectAll="false" enableDelete="true">
        </table>
        <!--<table tableName="draft_maker" domainObjectName="DraftMaker" enableSelectAll="false" enableDelete="true">-->
        <!--</table>-->
        <!--<table tableName="draft_maker_works" domainObjectName="DraftMakerWorks" enableSelectAll="false" enableDelete="true">-->
        <!--</table>-->
        <!--<table tableName="experts" domainObjectName="Experts" enableSelectAll="false" enableDelete="true">-->
        <!--</table>-->
        <!--<table tableName="account" domainObjectName="Account" enableSelectAll="false" enableDelete="true">-->
        <!--</table>-->
        <table tableName="member" domainObjectName="Member" enableSelectAll="false" enableDelete="false">
        </table>
        <table tableName="draft_collect_type" domainObjectName="DraftCollectType" enableSelectAll="false"
               enableDelete="false">
        </table>
        <table tableName="draft_response" domainObjectName="DraftResponse" enableSelectAll="false" enableDelete="false">
        </table>

        <table tableName="draft_attachment" domainObjectName="DraftAttachment" enableSelectAll="false"
               enableDelete="true">
        </table>

        <!-- Table config end -->
    </context>
</generatorConfiguration>